{"ast":null,"code":"import axios from 'axios';\nimport ProductBox from '../components/ProductBox.vue';\nexport default {\n  name: 'HomeView',\n\n  data() {\n    return {\n      latestProducts: []\n    };\n  },\n\n  components: {\n    'app-product-box': ProductBox\n  },\n  methods: {\n    async getLatestProducts() {\n      this.$store.commit('setIsLoading', true);\n      await axios.get('api/v1/latest-products/').then(response => {\n        console.log(response.data);\n        this.latestProducts = response.data;\n      }).catch(err => {\n        console.log('Error request home components');\n        console.log(err);\n      });\n      this.$store.commit('setIsLoading', false);\n    }\n\n  },\n\n  created() {\n    this.getLatestProducts();\n    window.document.title = 'Home | Djackets';\n  }\n\n};","map":{"version":3,"mappings":"AAyBE,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,8BAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,cAAc,EAAC;IADX,CAAN;EAGD,CANY;;EAObC,UAAU,EAAE;IACV,mBAAkBJ;EADR,CAPC;EAUbK,OAAO,EAAC;IACN,MAAMC,iBAAN,GAAyB;MACvB,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,cAAnB,EAAkC,IAAlC;MACA,MAAMT,KAAK,CAACU,GAAN,CAAU,yBAAV,EACHC,IADG,CACGC,QAAD,IAAY;QAChBC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACT,IAArB;QACA,KAAKC,cAAL,GAAsBQ,QAAQ,CAACT,IAA/B;MACD,CAJG,EAKHY,KALG,CAKIC,GAAD,IAAO;QACZH,OAAO,CAACC,GAAR,CAAY,+BAAZ;QACAD,OAAO,CAACC,GAAR,CAAYE,GAAZ;MACD,CARG,CAAN;MASA,KAAKR,MAAL,CAAYC,MAAZ,CAAmB,cAAnB,EAAkC,KAAlC;IACF;;EAbM,CAVK;;EAyBbQ,OAAO,GAAE;IACP,KAAKV,iBAAL;IACAW,MAAM,CAACC,QAAP,CAAgBC,KAAhB,GAAwB,iBAAxB;EACF;;AA5Ba,CAAf","names":["axios","ProductBox","name","data","latestProducts","components","methods","getLatestProducts","$store","commit","get","then","response","console","log","catch","err","created","window","document","title"],"sourceRoot":"","sources":["C:\\Users\\user\\Desktop\\django_vue_ecommerce\\djackets_vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <section class=\"hero is-medium is-dark mb-6\">\n      <div class=\"hero-body has-text-centered\">\n          <p class=\"title mb-6\">\n              Welcome to Djacket\n          </p>\n          <p class=\"subtitle\">\n              The best jacket store online\n          </p>\n      </div>\n    </section>\n\n      <div class=\"columns is-multiline\">\n        <div class=\"column is-12\">\n            <h2 class=\"is-size-2 has-text-centered\">Latest products</h2>\n        </div>\n        <!-- !ProductBox Component -->\n        <app-product-box v-for=\"lp in latestProducts\" :key=\"lp.id\"></app-product-box>\n      </div>\n\n  </div>\n</template>\n\n<script>\n  import axios from 'axios'\n  import ProductBox from '../components/ProductBox.vue'\n\n  export default {\n    name: 'HomeView',\n    data(){\n      return{\n        latestProducts:[]\n      }\n    },\n    components: {\n      'app-product-box':ProductBox\n    },\n    methods:{\n      async getLatestProducts(){\n        this.$store.commit('setIsLoading',true)\n        await axios.get('api/v1/latest-products/')\n          .then((response)=>{\n            console.log(response.data)\n            this.latestProducts = response.data\n          })\n          .catch((err)=>{\n            console.log('Error request home components')\n            console.log(err)\n          })\n        this.$store.commit('setIsLoading',false)\n      }\n    },\n    created(){\n      this.getLatestProducts()\n      window.document.title = 'Home | Djackets'\n    }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}